\<!DOCTYPE html>

<html>
    <head>
        <title>#{pageTitle pc}
        \^{const $ pageHead pc}
        \^{ga}
    <body>
        <div id="doc" .kui-t1 .clearfix>
          <div id="seitoku-header-logo">
            <img id="seitoku-header" src="@{StaticR img_seitoku_header_gif}">
          <div id="kui-main">
            <div id="maincol" .kui-b>
              \^{header}
              $maybe msg <- mmsg
                <div id="message">#{msg}
              \^{const $ pageBody pc}
              \^{footer}
          <div id="sidebar" .kui-b .clearfix>
            <div id="sidenavi" .sidebox .clearfix>
            <div id="recent-changes" .sidebox>
            <div id="tools" .sidebox>
              <fieldset>
                <legend>_{MsgTools}
                <form method="get" action="@{WikiListR}">
                  <input type="text" name="q" placeholder="_{MsgSearchPlaceholder}" .watermark>
                  <button type="submit" name="_method" value="search">_{MsgGo}
                <form method="get" action="http://www.google.com/search">
                  <input type="hidden" name="hq" value="inurl:#{googleInurl}">
                  <input type="hidden" name="ie" value="UTF-8">
                  <input type="hidden" name="oe" value="UTF-8">
                  <input type="hidden" name="filter" value="0">
                  <input type="text" name="q" .google>
                  <button type="submit" name="btnG">_{MsgGo}
        $maybe _ <- mu
          <div id="file_selector">
           <div id="photo-gallery" .scroll>
             <ul id="selectable_images" .inline>
           <hr>
           <div id="upload_error" .errors.hide>_{MsgFailToUploadFile}
           <form id="upload_form" method="post" enctype="multipart/form-data" action="@{UploadR}">
             <div id="fields" .inline>
               <input id="upload_file" type="file" name="upfile">
             <input id="go_upload" type="button" name="upload" value="_{MsgUploadFile}">
           <div id="trash-area" .right>
             <img id="trashbox" src="@{StaticR img_trash_empty64_png}" .transparent>
