body
    font-family: sans-serif
    margin: 0
    padding: 0

div#message
    font-size: 150%
    color: red
    margin: 3px
    padding: 10px
    border: solid 2px red
    backgroud-color: #ffc0a0

.errors
    color: red
    font-style: italic

.warnings
    color: red
    font-style: bold

.example
    border: dotted 1px gray
    background-color: #e0e0e0
    margin: 1em
    padding: 0.5em

.scroll
    overflow: scroll
    height: 360px

.kui-t1
   margin: auto
   text-align: left
   width: 57.69em

#doc
   margin: auto 10px;
   width: auto
   text-align: left

#kui-main
   float: right;
   width: 100%
   margin-left: -25em

.kui-b
   position: relative

.kui-t1 .kui-b
   float: left
   width: 12.3077em

.kui-t1 #kui-main .kui-b
   margin-left: 13.3077em

#kui-main .kui-b
   float: none
   width: auto
   position: static

#userbox, #file-list-view
   font-size: 93%
   color: #888
   font-weight: bold
   text-align: right

#userbox
   margin-top: 1em

#userbox a, #file-list-view a
   color: #8080ff

ul.tabs
   margin: 0 0 1px
   padding: 0

ul.tabs li.selected
   border-bottom: 3px solid white

ul.tabs li
   background: none repeat scroll 0 0 white
   border-color: #cccccc
   border-style: solid solid none
   border-width: 1px 1px medium
   display: inline
   margin: 0 0 0 1.2em
   padding: 0 0.6em

ul.tabs li a
   color: #3366CC
   font-size: 93%
   font-weight: bold
   margin: 0px
   text-decoration: none

#main-content
   background-color: #ffffff
   border: 1px solid #CCCCCC
   font-size: 108%
   padding: 1em

.sidebox
   margin-top: 25px

ul, ol
   -webkit-padding-start: 15px

ul
   list-style-type: square

img
   border-style: none

#footer
   color: #888888
   font-size: 93%
   padding: 1em
   text-align: center

#photo-galary
   text-align: center
   vertical-align: middle

ul#selectable_images
   display: inline-block
   list-style: none
   margin: 5px 0px

ul#selectable_images li
   float: left
   margin-left: 8px
   margin-right: 8px
   margin-top: 10px
   padding: 0px
   position: relative

ul#selectable_images li a
   position: relative

ul#selectable_images li a img
   display: block
   margin: 0px
   overflow: hidden
   padding: 0px
   position: relative

.image-galary
   text-align: center
   vertical-align: middle
   display: inline-block
   margin: 2px

.image-galary a
   color: white
   background-color: white
   border-style: none
